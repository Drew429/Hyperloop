//---------------------------------------------------------------------------
//                       OTAC Turbine V&V test case
//                         based on TURBAN input
//---------------------------------------------------------------------------


//---------------------------------------------------------------------------
//                          SET THE THERMO PACKAGE
//---------------------------------------------------------------------------
setThermoPackage( "Janaf" );


Element FlowStart tube {
   W = 1.582 "kg/sec";
   Pt = 99.0/0.5282 "Pa";
   Tt = 292./0.8333 "K";
   Fl_O.MN = 1.00;
}

Element Splitter tubeBypass {
   BPRdes = 2.2285;
   Fl_O1.MN = 1.0;
   Fl_O2.MN = 1.0;
} 

Element FlowEnd endA { 
} 

Element Inlet inlet {
   eRamBase = 1.000;
   Fl_O.MN = 0.60;
} 

Element Compressor compressorA {
   PRdes = 12.47;
   effDes = 0.80;
   Ndes = 1.000;
   Fl_O.MN = 0.40;
} 

Element Duct intercoolerA {
   dPqP_dmd = 0.0;
   switchQ = "INPUT";
   Q_in = -237.8;
   Fl_O.MN = 0.40;
   //void preexecute() {
   //   Q_in = -steam.Qin;
   //} 
} 

Element Splitter splitter {
   BPRdes = 1.45;
   Fl_O1.MN = 0.40;
   Fl_O2.MN = 0.40;
} 

Element Nozzle nozzle {
   switchType = "CON_DIV";
   PsExhName = "tube.Fl_O.Ps";
}

Element FlowEnd endB {
} 

Element Compressor compressorB {
   PRdes = 5.00;
   effDes = 0.8000;
   Ndes = 1.000;
   Fl_O.MN = 0.40;
} 
