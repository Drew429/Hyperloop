   W = 0.14 "kg/sec";
   Pt = 101000. "Pa";
   Tt = 292. "K";
   Fl_O.MN = 0.40;
   
   // constituency = "H2O"
   WAR = 1.0;  // why is WAR limit = 1 ????

   // real Qin, delHvap = 2257.0 kJ/kg, 970.99 BTU/lb @ Tboiling
